<!DOCTYPE html>
<html>
  <head>
    <%= render 'shared/head' %>
  </head >

  <body>
    <% if yield(:fullpath).present? %>
      <%= yield(:fullpath) %>
    <% else %>
      <script>
        Appcontroller = "<%= request.original_fullpath %>"
      </script>
    <% end %>

    <%= render 'upload_start_page' %>
    <%= render 'shared/notice' %>

    <%= yield(:content_body)%>

    <% cache "footer_v1" do %>
      <%= render('shared/footer', color: 'inline-block link pr-2') %>
    <% end %>

    <%= render 'shared/photoswipe' %>
  </body>
</html>
