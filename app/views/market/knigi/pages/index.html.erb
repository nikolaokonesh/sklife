<h1>Market Knigi Pages</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Commentable</th>
      <th>Commentable type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @market_knigi_pages.each do |market_knigi_page| %>
      <tr>
        <td><%= market_knigi_page.title %></td>
        <td><%= market_knigi_page.commentable_id %></td>
        <td><%= market_knigi_page.commentable_type %></td>
        <td><%= link_to 'Show', market_knigi_page %></td>
        <td><%= link_to 'Edit', edit_market_knigi_page_path(market_knigi_page) %></td>
        <td><%= link_to 'Destroy', market_knigi_page, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

