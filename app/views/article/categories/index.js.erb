<% if params[:page] %>
  <% postak = SecureRandom.random_number(10000000000) %>
  var colum = document.querySelector("#categories-index")
  var pagin = document.querySelector("#categories-index-paginate")

  colum.innerHTML += ("<%= j render partial: 'article/categories/index', locals: {posts: @posts, postak: postak} %>")
  pagin.innerHTML = ("<%= j pag_helper(@posts, 'Далее') %>")

  var colore = document.querySelector(".open-posts-color-<%= postak %>")
  setTimeout(() => colore.classList.toggle("bg-green-200"), 1500)
<% end %>
