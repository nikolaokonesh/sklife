<p id="notice"><%= notice %></p>

<h1>Article Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Posttable</th>
      <th>Posttable type</th>
      <th>No comments</th>
      <th>Subscribe</th>
      <th>Upgrade</th>
      <th>User</th>
      <th>Type</th>
      <th>Slug</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @article_posts.each do |article_post| %>
      <tr>
        <td><%= article_post.title %></td>
        <td><%= article_post.posttable_id %></td>
        <td><%= article_post.posttable_type %></td>
        <td><%= article_post.no_comments %></td>
        <td><%= article_post.subscribe %></td>
        <td><%= article_post.upgrade %></td>
        <td><%= article_post.user_id %></td>
        <td><%= article_post.type %></td>
        <td><%= article_post.slug %></td>
        <td><%= link_to 'Show', article_post %></td>
        <td><%= link_to 'Edit', edit_article_post_path(article_post) %></td>
        <td><%= link_to 'Destroy', article_post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Article Post', new_article_post_path %>
